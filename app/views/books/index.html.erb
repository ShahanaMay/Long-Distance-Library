<h2>Books Currently in Your Library</h2>

<table>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author_last_name %>, <%= book.author_first_name %></td>
      <td><%= link_to "Edit", edit_book_path(book) %></td>
      <td><%= link_to "Delete", book_path(book) %></td>
    </tr>
  <% end %>
</table>

<div class="sidebar">
  <%= link_to "Create a New Book!", new_book_path %>
  <%= link_to "Back to the Beginning!", root_path %>
</div>
